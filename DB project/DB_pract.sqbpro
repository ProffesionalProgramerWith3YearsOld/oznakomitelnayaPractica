CREATE VIEW start_view AS
	SELECT id_employ,surname, name, patronymic, date_of_birth, managing_name,departament_name,record_date,position FROM Employ
		JOIN Employee_workbook_record ON Employ.id_employ = Employee_workbook_record.id_employ_fk
			JOIN Managing ON Employee_workbook_record.to_managing_fk = Managing.id_managing
				JOIN Departament ON Managing.id_departament_fk = Departament.id_departament</sql><current_tab id="1"/></tab_sql></sqlb_project>
